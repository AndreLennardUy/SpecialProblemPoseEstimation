    "# Function to calculate the angle between three points\n",
    "def calculate_angle(p1, p2, p3):\n",
    "    angle_radians = math.atan2(p3[1] - p2[1], p3[0] - p2[0]) - math.atan2(p1[1] - p2[1], p1[0] - p2[0])\n",
    "    angle_degrees = math.degrees(angle_radians)\n",
    "    return abs(angle_degrees)\n",
    "\n",
    "# Function to check if the angle between three points is within a tolerance\n",
    "def angle_within_tolerance(p1, p2, p3, target_angle, tolerance):\n",
    "    angle = calculate_angle(p1, p2, p3)\n",
    "    return abs(angle - target_angle) < tolerance\n",
    "\n",
    "# Function to check if the wrists, elbows, and shoulders are relatively aligned\n",
    "def wrists_elbows_shoulders_aligned(keypoints, confidence_threshold):\n",
    "    right_wrist = keypoints[0][0][10]\n",
    "    right_elbow = keypoints[0][0][8]\n",
    "    right_shoulder = keypoints[0][0][6]\n",
    "    left_wrist = keypoints[0][0][9]\n",
    "    left_elbow = keypoints[0][0][7]\n",
    "    left_shoulder = keypoints[0][0][5]\n",
    "\n",
    "    # Define target angle and tolerance for alignment\n",
    "    target_angle = 180  # Straight line\n",
    "    tolerance = 25  # Tolerance for alignment\n",
    "\n",
    "    # Check alignment for right side\n",
    "    right_aligned = angle_within_tolerance(right_wrist, right_elbow, right_shoulder, target_angle, tolerance)\n",
    "\n",
    "    # Check alignment for left side\n",
    "    left_aligned = angle_within_tolerance(left_wrist, left_elbow, left_shoulder, target_angle, tolerance)\n",
    "\n",
    "    return right_aligned and left_aligned\n",
    "\n",
    "# Function to check if the shoulders, hips, knees, and ankles form a diagonal\n",
    "def shoulders_hips_knees_ankles_diagonal(keypoints, confidence_threshold):\n",
    "    left_shoulder = keypoints[0][0][5]\n",
    "    left_hip = keypoints[0][0][11]\n",
    "    left_knee = keypoints[0][0][13]\n",
    "    left_ankle = keypoints[0][0][15]\n",
    "    right_shoulder = keypoints[0][0][6]\n",
    "    right_hip = keypoints[0][0][12]\n",
    "    right_knee = keypoints[0][0][14]\n",
    "    right_ankle = keypoints[0][0][16]\n",
    "\n",
    "    # Define target angle and tolerance for diagonal\n",
    "    target_angle = 135  # Diagonal angle\n",
    "    tolerance = 25  # Tolerance for diagonal\n",
    "\n",
    "    # Check diagonal for left side\n",
    "    left_diagonal = angle_within_tolerance(left_shoulder, left_hip, left_knee, target_angle, tolerance)\n",
    "\n",
    "    # Check diagonal for right side\n",
    "    right_diagonal = angle_within_tolerance(right_shoulder, right_hip, right_knee, target_angle, tolerance)\n",
    "\n",
    "    return left_diagonal and right_diagonal"
